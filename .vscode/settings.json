{
    "files.exclude": {
        "coverage.out": true,
        "**/main": true,
        "**/__debug_bin": true
    },
    "sql-formatter.dialect": "n1q1",
    "editor.rulers": [
        80,
        120
    ],
    "go.lintTool": "golangci-lint",
    "go.lintFlags": [
        "--fast"
    ],
    "cSpell.words": [
        "gocb",
        "healthcheck",
        "mpfd",
        "necsfield",
        "Presign",
        "Recaptcha",
        "Robohash",
        "Taymiyyah",
        "templater",
        "unfollow"
    ],
    "makefile.configureOnOpen": false
}
